{% extends "@VendorSkeleton/layout.html.twig" %}

{% block body %}
    {% if examples|length > 0 %}
        <ul>
            {% for example in examples %}
                <li><a href="{{ url('example_edit', {'id': example.id}) }}">{{ example.name }}</a></li>
            {% endfor %}
        </ul>
    {% endif  %}
    <a href="{{ url('example_edit') }}">{{ "example.add"|trans }}</a>
{% endblock body %}