{
    "name": "saxulum/saxulum",
    "license": "MIT",
    "type": "project",
    "description": "saxulum a silex skeleton, very symfony2 like",
    "autoload":
    {
        "psr-0":
        {
            "": "src/"
        }
    },
    "require": {
        "php": ">=5.3.3",
        "dflydev/doctrine-orm-service-provider": "~1.0.4",
        "dominikzogg/doctrine-orm-manager-registry-provider": "~1.3.3",
        "igorw/config-service-provider": "~1.2",
        "knplabs/knp-menu": "2.0.*@alpha",
        "leafo/lessphp": "~0.4.0",
        "monolog/monolog": "1.5.*",
        "saxulum/saxulum-assetic-twig-provider": "~1.2.0",
        "saxulum/saxulum-bundle-provider": "~1.0.1",
        "saxulum/saxulum-console": "~1.2.0",
        "saxulum/saxulum-route-controller-provider": "~1.0.5",
        "saxulum/saxulum-doctrine-orm-commands": "~1.0.0",
        "saxulum/saxulum-translation-provider": "~1.0.0",
        "saxulum/saxulum-webprofiler-provider": "~1.0",
        "silex/silex": "~1.1",
        "silex/web-profiler": "~1.0.2",
        "swiftmailer/swiftmailer": "~5.0",
        "symfony/doctrine-bridge": "~2.4",
        "symfony/config": "~2.4",
        "symfony/finder": "~2.4",
        "symfony/form": "~2.4",
        "symfony/security": "~2.4",
        "symfony/translation": "~2.4",
        "symfony/twig-bridge": "~2.4",
        "symfony/validator": "~2.4",
        "symfony/yaml": "~2.4",
        "twig/twig": "~1.2",

        "jquery/jquery": "~1.9.1",
        "twbs/bootstrap": "~2.3.2"
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "jquery/jquery",
                "version": "1.9.1",
                "dist": {
                    "url": "http://code.jquery.com/jquery-1.9.1.js",
                    "type": "file"
                }
            }
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
