{
    "name": "saxulum/saxulum",
    "license": "MIT",
    "type": "project",
    "description": "saxulum a silex skeleton, very symfony2 like",
    "autoload":
    {
        "psr-0":
        {
            "": "src/"
        }
    },
    "require": {
        "saxulum/saxulum-framework": "dev-master@dev",
        "jquery/jquery": "~1.9.1",
        "twbs/bootstrap": "~2.3.2"
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "jquery/jquery",
                "version": "1.9.1",
                "dist": {
                    "url": "http://code.jquery.com/jquery-1.9.1.js",
                    "type": "file"
                }
            }
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
